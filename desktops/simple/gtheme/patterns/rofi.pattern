<[output-file]>=~/.config/rofi/colors.rasi
// <[theme-name]>

* {
  font:       "<[default-font|FiraCode Nerd Font]> Medium 11";
  bg:         #<[background]>;
  fg:         #<[foreground]>;
  se:         #<[selection-background]>;
  red:        #<[red]>;
  green:      #<[green]>;
  yellow:     #<[yellow]>;
  blue:       #<[blue]>;
  magenta:    #<[magenta]>;
  cyan:       #<[cyan]>;
}
